{
  "name": "wallfall",
  "homepage": "https://flubbex.github.io/wallfall",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages":         "^1.2.0",
    "react":            "^16.4.1",
    "react-dom":        "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts":    "1.1.4"
  },
  "scripts": {
    "start":      "react-scripts start",
    "build":      "react-scripts build",
    "test":       "react-scripts test --env=jsdom",
    "eject":      "react-scripts eject",
    "predeploy":  "npm run build",
    "deploy":     "node node_modules/gh-pages/bin/gh-pages -d build",
    "iron":       "node ./iron",
    "pack":       "node ./iron pack",
    "publish":    "node ./iron publish"
  }
}
